/*C program on to find a Leap year*/
#include<stdio.h>
int main()
{
    int year;
    printf("\nenter the year: ");
    scanf("%d",&year);
    if(year%100==0)
    {
        if(year%400==0)
            printf("this is a leap year\n");
        else
            printf("this is not a leap year\n");
    }

    else
    {
        if(year%4==0)
            printf("this is a leap year\n");
        else
            printf("this is not a leap year\n");
    }
    return 0;

}
