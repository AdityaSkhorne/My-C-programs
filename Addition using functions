// Sending and receiving values between functions
#include<stdio.h>
int Cal_sum(int x,int y,int z);
int main()
{
    int a,b,c,sum;
    printf("enter any three numbers:- ");
    scanf("%d%d%d",&a,&b,&c);
    sum=Cal_sum(a,b,c);
    printf("sum=%d\n",sum);
    return 0;
}
int Cal_sum(int x,int y,int z)
{
    int d;
    d=x+y+z;
    return(d);
}
